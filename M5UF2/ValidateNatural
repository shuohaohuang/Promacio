using System;

namespace A
{
    class A
    {
        static void Main()
        {
            Console.WriteLine("Introdueix dos nombre");
            
            
            Natural(Convert.ToInt32(Console.ReadLine()));

            
            Natural(Convert.ToInt32(Console.ReadLine()));
        }

        public static void Natural(int a)
        {
            Console.WriteLine(a % 1 == 0 ? "És natural" : "No és natual");
        }
    }


